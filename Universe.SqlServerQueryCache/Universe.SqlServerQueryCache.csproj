  <PropertyGroup>
    <TargetFrameworks>net8.0;net6.0;net35;net40;net45;net46;net47;net48;netstandard2.0;</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
